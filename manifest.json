{
  "manifest_version": 2,

  "name": "soundcloud snapshot",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",


  "content_scripts": [
    {
      "matches": ["https://soundcloud.com/*"],
      "js": ["ui.js", "dl.js"],
      "css": ["ui.css"],
      "run_at": "document_end"
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
